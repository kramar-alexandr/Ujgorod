// only for 8.x
global
function string 255 StrReplace(string original,string target,string replacement) 
begin
  string 255 res;
  integer slen,i,tlen;
  string 10 chunk;
  string 1 tc;

  slen = Len(original);
  tlen = Len(target);
  tc = Left(target,1);
  for(i=0;i<slen;i=i+1) begin
    chunk = Mid(original,i,1);
    if(chunk == tc) then begin
      if(Mid(original,i,tlen)==target) then begin
        chunk = replacement;
        i = i + tlen - 1;
      end;
    end; 
    res = res & chunk;
  end;

  StrReplace = res;
  return;
end;

