	AddTextToArea("</sheetData>",tempArea);
	AppendAreaToFile(path & "/xl/worksheets/sheet" & SheetNum & ".xml",tempArea);
	RETURN;
END;

global
procedure BeginRow(integer SheetNum, string pathToXLSX, integer RowNum, integer outlineLevel, val height)
BEGIN
